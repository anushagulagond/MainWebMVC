@model IEnumerable<MainWebMVC.Models.EmpDept>

@{
    ViewData["Title"] = "EmpDeptView";
}

<h1>EmpDeptView</h1>

<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.EmpId)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.EmpName)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.DeptId)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.DeptName)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.DeptLoc)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.LibId)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.LibName)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.EmpId)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.EmpName)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.DeptId)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.DeptName)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.DeptLoc)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.LibId)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.LibName)
            </td>
           
        </tr>
}
    </tbody>
</table>
